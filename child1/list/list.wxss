/* pages/list/list.wxss */
.list {
  display: flex;
  padding: 10px;
  border: 3px solid #efefef;
  margin: 10rpx;
}

.list_image {
  width: 200rpx;
}

.list_title {
  padding-left: 40rpx;
  line-height: 200rpx;
  font-size: 40rpx;
}

/* 秦安简介标题 */
.qinantitle {
  text-align: center;
  font-size: 36rpx;
  border: 1px solid black;
  padding: 20px 10px;
}

/* 重写article样式 */
.article {
  padding: 0 29rpx;
  background-color: rgb(250, 249, 241);
  box-sizing: border-box;
  border-left: 30rpx solid rgb(169, 216, 238);
  border-right: 30rpx solid rgb(169, 216, 238);
  text-align: left;
}

